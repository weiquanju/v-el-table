{
  "name": "v-el-table",
  "version": "0.1.0-alpha.18",
  "description": "Easy to use table and form component of Vue 3",
  "author": "anbine@qq.com",
  "homepage": "https://github.com/weiquanju/v-el-table",
  "keywords": [
    "v-el-table",
    "table",
    "form",
    "element-plus",
    "ui",
    "vue"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "src/**/*.d.ts"
  ],
  "main": "./dist/v-el-table.umd.js",
  "module": "./dist/v-el-table.es.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/v-el-table.es.js",
      "require": "./dist/v-el-table.umd.js"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preserve": "vite build && cp ./types/* ./dist/",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "prepare": "husky install"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+github.com:weiquanju/v-el-table.git"
  },
  "bugs": {
    "url": "https://github.com/weiquanju/v-el-table/issues"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "element-plus": "^2.1.8",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.0.0",
    "typescript": "~4.3.5",
    "vite": "^2.6.3",
    "vue-tsc": "^0.3.0"
  }
}
