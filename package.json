{
  "name": "v-el-table",
  "version": "0.1.0-beta.43",
  "description": "Easy to use table and form component of Vue 3",
  "author": "anbine@qq.com",
  "homepage": "https://github.com/weiquanju/v-el-table",
  "keywords": [
    "v-el-table",
    "table",
    "form",
    "element-plus",
    "ui",
    "vue"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p ./build/tsconfig.json",
    "serve": "vite preview --port 5050",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "prepare": "husky install"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+github.com:weiquanju/v-el-table.git"
  },
  "bugs": {
    "url": "https://github.com/weiquanju/v-el-table/issues"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.4.4",
    "vue": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
